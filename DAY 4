FACTORIES:

package DAY4;

import java.util.Scanner;

public class factorize {

	public static void main (String[] args) {
	Scanner sc=new Scanner (System.in);
	int num=sc.nextInt();
	int fact=1;
	for(int i=1;i<=num;i++)
	{
		fact =fact*i;
	}
System.out.println(fact);

   }
}

EVENCOUNT:

package DAY4;

import java.util.Scanner;

public class evencount {

	public static void main (String[] args) {
     Scanner sc=new Scanner(System.in);
     int start=sc.nextInt();
     int end=sc.nextInt();
     int evencount=0;
     for(int i=start;i<=end;i++)
     {
    	 if(i%2==0)
     {
    	 evencount++;
    	}
    	 
     }
     {
    	 System.out.println(evencount);
     }
}
}

ODDCOUNT:

package DAY4;

import java.util.Scanner;

public class assignment {

	public static void main (String[] args) {
     Scanner sc=new Scanner(System.in);
     int start=sc.nextInt();
     int end=sc.nextInt();
     int oddcount=0;
     for(int i=start;i<=end;i++)
     {
    	 if(i%1==0)
     {
    	 oddcount++;
    	}
     }
     {
    	 System.out.println(oddcount);
     }
}
}

GCD:

package DAY4;

import java.util.Scanner;
import java.io.*;
import java.util.*;

public class hw {

	    public static void main(String[] args) {
	        Scanner sc = new Scanner(System.in);

	        int gokul = sc.nextInt();
	        int minor = sc.nextInt();
	        int totalPower = sc.nextInt();

	        int initialGokul = gokul;
	        int initialMinor = minor;
	        int initialPower = totalPower;

	        boolean gokulTurn = true;

	        while (totalPower > 0) {
	            int currentPower = gokulTurn ? gokul : minor;
	            int gcd = findGCD(currentPower, totalPower);

	            int output = (totalPower < gcd) ? currentPower : gcd;
	            totalPower -= output;

	            if (totalPower == 0) {
	                if (initialGokul == initialMinor)
	                    System.out.println("It's a draw!");
	                else
	                    System.out.println("Winner: " + (gokulTurn ? "Gokul" : "Minor"));
	                break;
	            }

	            gokulTurn = !gokulTurn;
	        }

	        sc.close();
	    }

	    static int findGCD(int a, int b) {
	        while (b != 0) {
	            int temp = b;
	            b = a % b;
	            a = temp;
	        }
	        return a;
	    }
	}



