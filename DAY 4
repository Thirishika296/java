AMSTRONG:

package DAY4;

import java.util.Scanner;

public class armstrong {
	
	    public static void main(String[] args) {
	        Scanner sc = new Scanner(System.in);

	        System.out.print("Enter a number: ");
	        int number = sc.nextInt();

	        int original = number;
	        int digits = 0;
	        int sum = 0;
	        int temp = number;
	        while (temp > 0) {
	            temp /= 10;
	            digits++;
	        }

	        temp = number;
	        while (temp > 0) {
	            int digit = temp % 10;
	            sum += Math.pow(digit, digits);
	            temp /= 10;
	        }

	        if (sum == original) {
	            System.out.println(original + " is an Armstrong number.");
	        } else {
	            System.out.println(original + " is NOT an Armstrong number.");
	        }

	        sc.close();
	    }
	}

REVERSE:

package DAY4;

import java.util.Scanner;

public class reverse {

	public static void main(String[] args) {
		 Scanner sc=new Scanner(System.in);
		 int num=sc.nextInt();
		 int rev=0;
		 while(num>0)
		 {
			 int temp = num%10;
			 rev= rev*10+temp;
			 num= num/10;
		 }
		 System.out.println(rev);
	}
}

WHILE:

package DAY4;

import java.util.Scanner;

public class While {

	public static void main(String[] args) 
	{
	 Scanner sc=new Scanner(System.in);
	 int num =5;
	 while(num<=20)
	 {
		 System.out.println(num);
		 num++;
	 }
}
}

WHILE REVERSE:

package DAY4;

import java.util.Scanner;

public class whilerev {

		public static void main(String[] args) 
		{
		 Scanner sc=new Scanner(System.in);
		 int num =5;
		 while(num<0);
		 {
			 System.out.println(num);
			 num--;
		 }
	}
	}

SINGLE DIGIT:

package DAY4;

import java.util.Scanner;

public class singledigit {

	public static void main(String[] args) {
	 Scanner sc=new Scanner(System.in);
	 int num=sc.nextInt();
	 while(num >9)
	 {
		 int sum=0;
		 while(num>0)
		 {
			 int temp= num%10;
			 sum= sum+temp;
			 num = num/10;
		}
		 num = sum;
	 }
	 System.out.println(num);

	}

}

PRIME:

package DAY4;

import java.util.Scanner;

public class prime {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num= sc.nextInt();
		int count=0;
		if(num ==0 || num==1)
		{
			System.out.println("not a prime");
		}
		else
		{
			for(int i=2;i<=num;i++)
			{
				if(num%i ==0)
				{
					count++;	
				}
		    }
		}
		if(count==1)
		{
			System.out.println("prime");
		}
		else
		{
			System.out.println("not a prime");
		}
	}
}

GCD:

package DAY4;

import java.util.Scanner;

public class GCD {

	
	public static void main(String[] args) {
	  Scanner sc=new Scanner(System.in);
	  int num= sc.nextInt();
	  int num2=sc.nextInt();
	  int gcd=0;
	  int min= (num<num2)? num:num2;
	  for(int i= min;i>0;i--)
	  {
		  if(num %i ==0 && num2%i==0)
		  {
			  gcd=i;
			  break;
		  }
	  }
	  System.out.println(gcd);
	}

}

FIBONNANCE:

package DAY4;

import java.util.Scanner;

public class Fib{
	
 public static void main (String[] args) {
	    Scanner sc= new Scanner(System.in);

	    System.out.print("Enter the number of terms for the Fibonacci series: ");
	        int n = scanner.nextInt();

	        if (n <= 0) {
	            System.out.println("Please enter a positive integer for the number of terms.");
	        } else {
	            int firstTerm = 0;
	            int secondTerm = 1;

	            System.out.println("Fibonacci Series up to " + n + " terms:");

	            if (n >= 1) {
	                System.out.print(firstTerm);
	            }
	            if (n >= 2) {
	                System.out.print(" " + secondTerm);
	            }

	            for (int i = 2; i < n; i++) {
	                int nextTerm = firstTerm + secondTerm;
	                System.out.print(" " + nextTerm);
	                firstTerm = secondTerm;
	                secondTerm = nextTerm;
	            }
	            System.out.println(); 
	        }

	        scanner.close();
	    }
	}

FC:

package DAY4;

import java.util.Scanner;

public class fc {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num= sc.nextInt();
		int count=0;
		for(int i=1;i<=num;i++)
		{
			if(num%i==0)		
			{
				count++;
			}
		}
	System.out.println(count);
				
	}
}

